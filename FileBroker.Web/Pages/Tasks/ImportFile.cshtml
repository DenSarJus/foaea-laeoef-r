@page
@model FileBroker.Web.Pages.Tasks.ImportFileModel
@{
}

<h2>Import File</h2>

<form enctype="multipart/form-data" method="post">
    <dl>
        <dd>
            <input asp-for="FormFile" type="file">
            <span asp-validation-for="FormFile"></span>
        </dd>
    </dl>
    <input asp-page-handler="Upload" class="btn btn-action" type="submit" value="Upload" />
</form>

<h3>@Model.Message</h3>
